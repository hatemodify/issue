<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
  </head>

  <body>
    <ul>
      <% boards.forEach(function(board) { %>
      <li>
        <a class="post-subject" href="/view/<%= board._id %>">
          <%= board.tit %>
        </a>

        <%= board.createdDate %>
      </li>
      <% }) %>
    </ul>
  </body>

</html>